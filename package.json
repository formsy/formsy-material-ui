{
  "name": "formsy-material-ui",
  "version": "0.6.3",
  "description": "A formsy-react compatibility wrapper for Material-UI form components.",
  "main": "./lib/index.js",
  "scripts": {
    "build": "babel ./src -d ./lib",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md}\"",
    "lint": "tsdx lint",
    "prepublish": "npm run build",
    "test": "PHANTOMJS_BIN=$(npm bin)/phantomjs $(npm bin)/karma start --single-run",
    "test:watch": "PHANTOMJS_BIN=$(npm bin)/phantomjs $(npm bin)/karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/formsy/formsy-material-ui.git"
  },
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "keycode": "^2.1.9"
  },
  "peerDependencies": {
    "create-react-class": "^15.6.0",
    "formsy-react": "^0.19.0",
    "material-ui": "^0.18.6",
    "prop-types": "^15.5.7",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^4.1.0",
    "chai-enzyme": "^0.8.0",
    "cheerio": "^0.22.0",
    "enzyme": "^2.9.1",
    "formsy-react": "^0.19.0",
    "json-loader": "^0.5.4",
    "karma": "^1.7.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-phantomjs-shim": "^1.2.0",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "material-ui": "^0.18.6",
    "mocha": "^3.4.2",
    "phantomjs-prebuilt": "^2.1.4",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-tap-event-plugin": "^2.0.1",
    "react-test-renderer": "^15.6.1",
    "sinon": "^2.3.8",
    "sinon-chai": "^2.12.0",
    "tsdx": "^0.14.1",
    "webpack": "^3.3.0"
  },
  "keywords": [
    "formsy",
    "formsy-react",
    "form",
    "MUI",
    "Material-UI",
    "React",
    "jsx",
    "react-component"
  ],
  "author": "Matt Brookes, Ryan Blakeley, and the Formsy Authors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/formsy/formsy-material-ui/issues"
  },
  "homepage": "https://github.com/formsy/formsy-material-ui#readme",
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "proseWrap": "always",
    "trailingComma": "all"
  }
}
