{
  "name": "formsy-material-ui",
  "version": "0.6.3",
  "description": "A formsy-react compatibility wrapper for Material-UI form components.",
  "main": "dist/index.js",
  "module": "dist/formsy-material-ui.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsdx build",
    "build:clean": "rm -r dist/* || true",
    "changelog": "auto-changelog",
    "deploy": "np",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md}\"",
    "lint": "tsdx lint",
    "prepublishOnly": "tsdx build",
    "preversion": "npm run lint",
    "start": "tsdx watch",
    "test": "tsdx test",
    "version": "npm run build && git add dist && npm run changelog && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/formsy/formsy-material-ui.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "keycode": "^2.1.9"
  },
  "peerDependencies": {
    "create-react-class": "^15.6.0",
    "formsy-react": "^0.19.0",
    "material-ui": "^0.18.6",
    "prop-types": "^15.5.7",
    "react": "^16.0.0 || ^17.0.0",
    "react-dom": "^16.0.0 || ^17.0.0"
  },
  "devDependencies": {
    "@types/create-react-class": "^15.6.3",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "create-react-class": "^15.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "formsy-react": "^0.19.0",
    "json-loader": "^0.5.4",
    "material-ui": "^0.18.6",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "tsdx": "^0.14.1"
  },
  "keywords": [
    "form",
    "formsy",
    "formsy-react",
    "jsx",
    "Material-UI",
    "MUI",
    "React",
    "react-component"
  ],
  "author": "Matt Brookes, Ryan Blakeley, and the Formsy Authors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/formsy/formsy-material-ui/issues"
  },
  "homepage": "https://github.com/formsy/formsy-material-ui#readme",
  "prettier": {
    "printWidth": 120,
    "proseWrap": "always",
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.tsx",
      "src/**/*.ts"
    ],
    "setupFilesAfterEnv": [
      "./setupTests.ts"
    ]
  }
}
